				function viewPhoto(current, count, akamai_url)
				{
					// change main pic's properties
					document.getElementById("story_photo").alt = arrAlt[current];
					document.getElementById("story_photo").title = arrCaption[current];
					document.getElementById("storyphotocaption").innerHTML  = arrCaption[current];
					// akamaize image URL if passed in
					if (akamai_url != "")
						document.getElementById("story_photo").src = akamai_url + "/" + arrPhoto[current];
					else
						document.getElementById("story_photo").src = arrPhoto[current];
					// credit/distributor
					if(arrCredit[current] != '')
					{
						if(arrDistributor[current] != '')
							document.getElementById("storyphotocredit").innerHTML  = "<b>Photograph by: </b>" + arrCredit[current] + ", " + arrDistributor[current];
