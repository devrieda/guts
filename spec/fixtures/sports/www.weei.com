                                                                                    <div id="twitter-button" class="social-option"><a href="http://twitter.com/share" class="twitter-share-button" data-count="horizontal" data-via="WEEI">Tweet</a><script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script></div>
                                                                                    <div id="plus-one" class="social-option"><g:plusone size="medium"></g:plusone></div>
                                                                                    
                                                                            </div>
									<div class="fleft tji_entry_footer">
										<script type="text/javascript">
											var idcomments_acct = 'f2a0e39eefd29031dbfc337376a93230';
