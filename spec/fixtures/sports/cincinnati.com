	*/
		var _s = GEL.ement("SCRIPT", { src: "http://news.cincinnati.com/odygel/lib/vendor/omniture/sitecatalyst.js?odyv=1.14.1.2935" }); 
	_s.setAttacher("inline"); 
	_s.attach(); 
		var _d = GEL.ement("SCRIPT", { src: "/odygel/lib/vendor/audiencemanager/dil.js?odyv=1.14.1.2935" }); 
