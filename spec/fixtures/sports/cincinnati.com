								   if (_s[i]){
										seg_pt.push(_s[i])
									}
								}
								t.push(seg_pt.join(''));
							}
							return t.join("_");
						}
						var 
						retval,
						ult_cookie = GEL.firefly.atyponAPI.cookie.get('userLicenseType');
						if (ult_cookie){
							retval = ult_cookie;
