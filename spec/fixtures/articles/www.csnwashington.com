      <div class="view-footer">
      <iframe allowtransparency="true" frameborder="0" scrolling="no" src="//platform.twitter.com/widgets/follow_button.html?screen_name=BenStandig&show_count=false" style="width:300px; height:20px;"></iframe>    </div>
  
  
